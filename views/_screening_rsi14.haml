%table.table.table-striped
  %thead
    %tr
      %th 銘柄
      %th 企業名
      %th RSI14
      %th 終値
      %th 変化
      %th 比率
  %tbody
    - if @screening_rsi14.length > 0
      - @screening_rsi14.each do |line|
        - code, open, high, low, close, diff, ratio, rsi14, name = line.strip.split("\t")
        - unless code == "Code"
          %tr
            %td
              %a{:href => "#{@root}stock/#{code}"}
                =h code
            %td
              %a{:href => "#{@root}stock/#{code}"}
                =h name
            %td
              =h rsi14
            %td
              =h number_with_delimiter(close)
            %td
              =h number_with_delimiter(diff)
            %td
              =h (ratio + "%")
