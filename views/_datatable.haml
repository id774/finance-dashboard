%table.table.table-striped#datatable
  %thead
    %tr
      %th 銘柄
      %th 企業名
      %th 始値
      %th 高値
      %th 安値
      %th 終値
      %th 差
      %th 比率
      %th RSI14
  %tbody
    - if @screening_rsi14.length > 0
      - @screening_rsi14.each do |line|
        - code, open, high, low, close, diff, ratio, rsi14, name = line.strip.split("\t")
        - unless code == "Code"
          %tr
            %td
              %a{:href => "#{@root}stock/#{code}"}
                =h code
            %td
              %a{:href => "#{@root}stock/#{code}"}
                =h name
            %td
              =h number_with_delimiter(open)
            %td
              =h number_with_delimiter(high)
            %td
              =h number_with_delimiter(low)
            %td
              =h number_with_delimiter(close)
            %td
              =h number_with_delimiter(diff)
            %td
              =h (ratio + "%")
            %td
              =h rsi14
